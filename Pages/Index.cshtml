@page
@model IndexModel
@{
    ViewData["Title"] = "Welcome";
}

<div class="text-center">
    <div id="slidesMainPage" class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
            <li data-target="#slidesMainPage" data-slide-to="0" class="active"></li>
            <li data-target="#slidesMainPage" data-slide-to="1"></li>
            <li data-target="#slidesMainPage" data-slide-to="2"></li>
        </ol>
        <div class="carousel-inner" style="max-height: 70vh">
            <div class="carousel-item active h-100">
                <img class="d-block w-100" src="images/ft_front.jpg" alt="First slide">
            </div>
            <div class="carousel-item h-100">
                <img class="d-block w-100" src="images/cherryd3.jpg" alt="Second slide">
            </div>
            <div class="carousel-item h-100">
                <img class="d-block w-100" src="images/website_unter_construction.jpg" alt="Third slide">
            </div>
        </div>
        <a class="carousel-control-prev" href="#slidesMainPage" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#slidesMainPage" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
</div>

</div>






